{
    "manifest_version": 3,
    "name": "Scrum Helper",
    "version": "1.0",
    "description": "Extenção para auxiliar na criação das histórias",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  